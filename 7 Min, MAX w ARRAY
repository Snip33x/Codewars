public class Kata
{
  public int Min(int[] list)
  {
    int min = list[0];

    for (int i = 0; i < list.Length; i++) {
        int number = list[i];

        if (number < min) {
            min = number;
        }
    }

    return min;
  }
  
  public int Max(int[] list)
  {
    int max = list[0];

    for (int i = 0; i < list.Length; i++) {
        int number = list[i];

        if (number > max) {
            max = number;
        }
    }

    return max;
  }
}
