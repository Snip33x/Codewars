using System;
using System.Linq;

public class Kata
{
  public static int PositiveSum(int[] arr)
  {
    int suma = 0;
    for(int i =0; i < arr.Length; i++)
    {
      if(arr[i] > 0)
        suma += arr[i];
    }
    if (suma > 0)
      return suma;
    else
      return 0;
  }
}

//Console.WriteLine(PositiveSum(new int[] { 3,4,6,3,4}));
